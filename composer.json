{
    "name": "sumpygump/qis-client",
    "description": "Console continuous integration for PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Jansen Price",
            "email": "jansen.price@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "sumpygump/qi-console": "1.*",
        "sumpygump/qi-db": "1.*",
        "phploc/phploc": "*",
        "phpmd/phpmd" : "2.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
